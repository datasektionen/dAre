<% provide(:title, "Edit administrator") %> 
<h1>Update your profile</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@administrator) do |f| %>
      <%= render 'shared/error_messages' %>

      <%= f.label :kth_id %>
      <%= f.text_field :kth_id %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
    
  </div>
</div>